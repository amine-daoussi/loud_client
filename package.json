    ]
  },
  "babel": {
    "exclude": [],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "babel-polyfill",
      "@testing-library/jest-dom/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.{js,jsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
